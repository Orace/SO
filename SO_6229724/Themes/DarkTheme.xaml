<so6229724:ThemeResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                                   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                                   xmlns:so6229724="clr-namespace:SO_6229724"
                                   DisplayName="The Dark Mode">
    
    <Style TargetType="{x:Type StackPanel}">
        <Setter Property="Background" Value="DarkSlateGray" />
    </Style>

    <Style TargetType="TextBlock">
        <Setter Property="FontFamily" Value="MS Gothic" />
    </Style>

</so6229724:ThemeResourceDictionary>